<?xml version="1.0" encoding="UTF-8"?>
<facelet-taglib
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facelettaglibrary_2_2.xsd"
	version="2.2"
>
	<description>Time ago JSF component</description>
	<namespace>http://tsotzolas.com/tags</namespace>
	<short-name>ts</short-name>
	<tag>
		<tag-name>jqslider</tag-name>
		<component>
			<component-type>jqSliderComponent</component-type>
		</component>
		<attribute>
			<name>mindate</name>
			<required>true</required>
			<type>java.util.Date</type>
		</attribute>
		<attribute>
			<name>maxdate</name>
			<required>true</required>
			<type>java.util.Date</type>
		</attribute>
		<attribute>
			<name>mindefaultdate</name>
			<required>false</required>
			<type>java.util.Date</type>
		</attribute>
		<attribute>
			<name>maxdefaultdate</name>
			<required>false</required>
			<type>java.util.Date</type>
		</attribute>
		<attribute>
			<name>for</name>
			<required>true</required>
			<type>java.lang.String</type>
		</attribute>
	</tag>
</facelet-taglib>